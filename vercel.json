{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "node_modules/ffmpeg-static/**",
                    "node_modules/ffprobe-static/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ]
}